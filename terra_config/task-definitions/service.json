[
{
      "name": "first",
      "image": "${var.registry_docker_image}",
      "cpu": 10,
      "memory": 128,
      "essential": true,
      "logConfiguration": {
         "logDriver": "awslogs",
         "options": {
              "awslogs-group": "ptt-alertor",
              "awslogs-region": "us-west-2"
         }
      },
      "portMappings": [
         {
         "containerPort": 9090,
         "hostPort": 80
         },
         {
         "containerPort": 6379,
         "hostPort": 6379
         }
      ],
      "mountPoints": [
         {
         "sourceVolume": "config",
         "containerPath": "/go/src/github.com/liam-lai/ptt-alertor/config",
         "readOnly": true
         }
      ]
}
]
